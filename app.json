{
    "expo": {
        "name": "wheele-mobile-frontend",
        "slug": "wheele-mobile-frontend",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/logo-b.png",
        "splash": {
            "image": "./assets/images/logo-b.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/logo-b.png",
                "backgroundColor": "#FFFFFF"
            }
        },
        "web": {
            "favicon": "./assets/images/logo-b.png"
        },
        "plugins": ["sentry-expo"],
        "hooks": {
            "postPublish": [{
                "file": "sentry-expo/upload-sourcemaps",
                "config": {
                    "organization": "wheel_e",
                    "project": "react-native-wheel-e",
                    "authToken": "1d87bfdec79311ec9a530e7f15d8d524"
                }
            }]
        }
    }
}